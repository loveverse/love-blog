{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "启动程序",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\server\\app.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node-terminal",
      "name": "运行脚本: test",
      "request": "launch",
      "command": "npm run test",
      "cwd": "${workspaceFolder}\\server"
    },
    {
      "type": "node-terminal",
      "name": "运行脚本: dev",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}\\server"
    }
  ]
}